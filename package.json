    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.5.0",
    "json-server": "^1.0.0-beta.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.18.0"
  }
